{
  "name": "vercel",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prebuild": "tailwindcss -o ./app/styles.css",
    "build": "remix build",
    "predeploy": "vercel pull --yes && vercel build",
    "deploy": "vercel deploy --prebuilt",
    "dev": "remix dev",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/vercel": "^1.6.4",
    "@vercel/node": "^1.15.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vercel": "^27.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.4",
    "@remix-run/eslint-config": "^1.6.4",
    "@remix-run/serve": "^1.6.4",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "workspaces": {
    "nohoist": [
      "**/*"
    ]
  }
